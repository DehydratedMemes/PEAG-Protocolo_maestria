@article{zhangEfficientEvaluationElectrostatic2021,
  title = {Efficient Evaluation of Electrostatic Potential with Computerized Optimized Code},
  author = {Zhang, Jun and Lu, Tian},
  date = {2021},
  journaltitle = {Phys. Chem. Chem. Phys.},
  volume = {23},
  number = {36},
  pages = {20323--20328},
  issn = {1463-9076, 1463-9084},
  doi = {10/gnm2bq},
  url = {http://xlink.rsc.org/?DOI=D1CP02805G},
  urldate = {2021-11-29},
  abstract = {The computerized optimized code enables Multiwfn to carry out large scale electronstatic potential analyses for systems of considerable size.           ,              The evaluation of molecular electrostatic potential (ESP) is a performance bottleneck for many computational chemical tasks like restrained ESP charge fitting or quantum mechanics/molecular mechanics simulations. In this paper, an efficient algorithm for the evaluation of ESP is proposed. It regroups the expression in terms of primitive Gaussian type orbitals (GTOs) with identical angular momentum types and nuclei centers. Each term is calculated using a computerized optimized code. This algorithm was integrated into the wavefunction analysis program Multiwfn and was tested on several large systems. In the cases of dopamine and remdesivir, the performance of this algorithm was comparable to or better than some popular state-of-the-art codes. For meta1–organic framework-5, where the number of GTOs and ESP points is 4840 and 259\,262, respectively, our code could finish the evaluation in 1874 seconds on ordinary hardware. It also exhibits good parallelization scaling. The source code of this algorithm is freely available and can become a useful tool for computational chemists.},
  langid = {english},
  annotation = {ZSCC: 0000001   citations (Semantic Scholar/DOI) [2021-11-29] ECC: 0000001  ZSCC: 0000001},
  file = {/Users/pabloalanis/Zotero/storage/HF389MXP/Efficient evaluation of electrostatic potential with computerized optimized code.pdf}
}

@article{zhangPhaseDiagramKNO32004,
  title = {The Phase Diagram of {{KNO3}}–{{KClO3}}},
  author = {Zhang, Xuejun and Tian, Jun and Xu, Kangcheng and Gao, Yici},
  date = {2004-02},
  journaltitle = {Thermochimica Acta},
  volume = {411},
  number = {1},
  pages = {21--26},
  issn = {00406031},
  doi = {10/btqz77},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0040603103004088},
  urldate = {2023-11-27},
  abstract = {The binary phase diagram of KNO3–KClO3 is studied by means of differential scanning calorimetry (DSC) and high-temperature X-ray diffraction. The limited solid solutions, K(NO3)1−x(ClO3)x (0 {$<$} x {$<$} 0.20) and K(NO3)1−x(ClO3)x (0.90 {$<$} x {$<$} 1.0), were formed in the KNO3-based solid solutions and KClO3-based solid solutions phase, respectively. For KNO3-based solid solutions, KNO3 ferroelectric phase can be stable from 423 to 223 K as a result of substituting of NO3 by ClO3-radicals. The temperatures for solidus and liquidus have been determined based on limited solid solutions. Two models, Henrian solution and regular solution theory for KNO3-based (␣) phase and KClO3-based (␤) phase, respectively, are employed to reproduce solidus and liquidus of the phase diagram. The results are in good agreement with the DSC data. The thermodynamic properties for ␣ and ␤ solid solutions have been derived from an optimization procedure using the experimental data. The calculated phase diagram and optimized thermodynamic parameters are thermodynamically self-consistent.},
  langid = {english},
  file = {/Users/pabloalanis/Zotero/storage/6MD9EK56/Zhang et al. - 2004 - The phase diagram of KNO3–KClO3.pdf}
}

@article{zhaoMultielementXrayMovie2019,
  title = {Multi-Element {{X-ray}} Movie Imaging with a Visible-Light {{CMOS}} Camera},
  author = {Zhao, Wenyang and Sakurai, Kenji},
  date = {2019-01-01},
  journaltitle = {J Synchrotron Rad},
  volume = {26},
  number = {1},
  pages = {230--233},
  issn = {1600-5775},
  doi = {10/ghf2wc},
  url = {https://scripts.iucr.org/cgi-bin/paper?S1600577518014273},
  urldate = {2023-10-25},
  abstract = {For many years, X-ray movies have been considered a promising tool for exploring and providing insights into chemical reactions. A simultaneous multi-element X-ray movie can further clarify the behavior difference of various elements and help investigate their interactions. The present short communication illustrates how to conduct multi-element X-ray movie imaging in a synchrotron facility solely by placing a micro-pinhole in front of a visible-light complementary metal-oxide semiconductor (CMOS) camera. It has been found that the CMOS camera can resolve X-ray fluorescence spectra when it is specially operated. In this work, a spatial resolution of ∼15\hspace{0.25em}µm was achieved. In the X-ray movie, a movie frame acquisition time of 2\hspace{0.25em}min and a spatial resolution of ∼50\hspace{0.25em}µm were simultaneously achieved. It is clear that the CMOS camera can be a cost-efficient option for many researchers who wish to establish their own setup for visualizing chemical diffusion in various reactions.}
}

