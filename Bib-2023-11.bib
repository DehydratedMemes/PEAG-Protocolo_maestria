@article{karleNaphthalenefunctionalizedResorcinareneSelective2022,
  title = {Naphthalene-Functionalized Resorcinarene as Selective, Fluorescent Self-Quenching Sensor for Kynurenic Acid},
  author = {Karle, Anna and Twum, Kwaku and Sabbagh, Noorhan and Haddad, Alise and Taimoory, S. Maryamdokht and Szczęśniak, Małgorzata M. and Trivedi, Evan and Trant, John F. and Beyeh, Ngong Kodiah},
